  <product id="deskzilla">
    <distjar jar="almworks-tracker-api.jar">
      <place dir="lib/"/>
      <include module="ExternalApi"/>
      <manifest/>
    </distjar>

    <distjar jar="deskzilla.jar">
      <place dir=""/>
      <include module="Launcher"/>
      <include module="AppInit"/>
      <include module="Launcher.Deskzilla"/>
      <manifest>
        <attribute name="Main-Class" value="com.almworks.launcher.Launcher"/>
      </manifest>
    </distjar>

    <distjar jar="base.jar">
      <place dir="components/"/>
      <include module="Application"/>
      <include module="Engine"/>
      <include module="Utils"/>
      <include module="CommonUtils"/>
      <include module="CoreComponents"/>
      <include module="Services"/>
      <include module="ConnectorSupport"/>
      <include module="ExternalApiService"/>
      <include module="Resources.Common"/>
      <include module="Resources.Deskzilla"/>
      <include module="Screenshot"/>
      <include module="widgets"/>
      <include module="ItemStorage"/>
      <include module="ItemSync"/>
      <include module="SpellCheck"/>
      <include module="FXUtils"/>
    </distjar>

    <distjar jar="bugzilla.jar">
      <place dir="components/"/>
      <include module="Bugzilla"/>
    </distjar>

    <distjar jar="twocents.jar">
      <place dir="lib/"/>
      <include module="twocents"/>
    </distjar>

