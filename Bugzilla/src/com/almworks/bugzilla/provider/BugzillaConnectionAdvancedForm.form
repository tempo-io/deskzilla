<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.almworks.bugzilla.provider.BugzillaConnectionAdvancedForm">
  <grid id="27dc6" binding="myWholePanel" layout-manager="FormLayout">
    <rowspec value="center:d:noGrow"/>
    <rowspec value="top:3dlu:noGrow"/>
    <rowspec value="center:d:noGrow"/>
    <rowspec value="top:6dlu:noGrow"/>
    <rowspec value="center:d:noGrow"/>
    <rowspec value="top:3dlu:noGrow"/>
    <rowspec value="center:d:noGrow"/>
    <rowspec value="top:6dlu:noGrow"/>
    <rowspec value="center:max(d;4px):noGrow"/>
    <rowspec value="top:3dlu:noGrow"/>
    <rowspec value="center:d:noGrow"/>
    <colspec value="fill:24px:noGrow"/>
    <colspec value="fill:max(d;4px):grow"/>
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <component id="faf08" class="javax.swing.JTextField" binding="mySuffixField">
        <constraints>
          <grid row="2" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
            <preferred-size width="150" height="-1"/>
          </grid>
          <forms defaultalign-horz="false"/>
        </constraints>
        <properties>
          <text value=""/>
        </properties>
      </component>
      <component id="13652" class="javax.swing.JCheckBox" binding="mySuffixCheck">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
          <forms defaultalign-horz="false"/>
        </constraints>
        <properties>
          <text value="Bugzilla is configured to use this &amp;e-mail suffix:"/>
        </properties>
      </component>
      <component id="503ca" class="javax.swing.JCheckBox" binding="myCharsetCheck">
        <constraints>
          <grid row="4" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
          <forms defaultalign-horz="false"/>
        </constraints>
        <properties>
          <text value="Use this &amp;charset when interacting with Bugzilla over HTTP:"/>
        </properties>
      </component>
      <component id="f660b" class="javax.swing.JComboBox" binding="myCharsetCombo">
        <constraints>
          <grid row="6" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="2" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
          <forms/>
        </constraints>
        <properties/>
      </component>
      <component id="4d8e3" class="com.almworks.util.components.AComboBox" binding="myTimezoneCombo">
        <constraints>
          <grid row="10" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="2" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
          <forms/>
        </constraints>
        <properties/>
      </component>
      <component id="4591f" class="javax.swing.JLabel" binding="myTimezoneLabel">
        <constraints>
          <grid row="8" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
          <forms defaultalign-horz="false"/>
        </constraints>
        <properties>
          <text value="Use this &amp;time zone when Bugzilla reports time without time zone: "/>
        </properties>
      </component>
    </children>
  </grid>
</form>
