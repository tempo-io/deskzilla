<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.almworks.spellcheck.SpellCheckSettings">
  <grid id="27dc6" binding="myWholePanel" default-binding="true" layout-manager="FormLayout">
    <rowspec value="center:d:noGrow"/>
    <rowspec value="top:4dlu:noGrow"/>
    <rowspec value="center:max(d;4px):noGrow"/>
    <rowspec value="top:4dlu:noGrow"/>
    <rowspec value="center:max(d;4px):noGrow"/>
    <rowspec value="top:4dlu:noGrow"/>
    <rowspec value="center:max(d;4px):noGrow"/>
    <colspec value="fill:max(d;4px):noGrow"/>
    <colspec value="left:4dlu:noGrow"/>
    <colspec value="fill:d:grow"/>
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <component id="c042b" class="javax.swing.JCheckBox" binding="myEnableCheck">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="3" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
          <forms/>
        </constraints>
        <properties>
          <text resource-bundle="com/almworks/spellcheck/message" key="spellcheck.settings.enableChecker.text"/>
        </properties>
      </component>
      <component id="cc53" class="javax.swing.JLabel" binding="myDictionariesLabel">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="3" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
          <forms/>
        </constraints>
        <properties>
          <labelFor value="e403a"/>
          <text resource-bundle="com/almworks/spellcheck/message" key="spellcheck.settings.dictionaries.label.text"/>
        </properties>
      </component>
      <scrollpane id="9a569">
        <constraints>
          <grid row="4" column="0" row-span="1" col-span="3" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
          <forms/>
        </constraints>
        <properties/>
        <border type="none"/>
        <children>
          <component id="e403a" class="com.almworks.util.components.ACheckboxList" binding="myDictionaries">
            <constraints/>
            <properties/>
          </component>
        </children>
      </scrollpane>
      <component id="2c632" class="javax.swing.JCheckBox" binding="myUseUser">
        <constraints>
          <grid row="6" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
          <forms/>
        </constraints>
        <properties>
          <text resource-bundle="com/almworks/spellcheck/message" key="spellcheck.settings.userDictionary.enable.text"/>
        </properties>
      </component>
      <component id="e067a" class="com.almworks.util.components.FileSelectionField" binding="myUserDictionaryFile">
        <constraints>
          <grid row="6" column="2" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
          <forms/>
        </constraints>
        <properties/>
